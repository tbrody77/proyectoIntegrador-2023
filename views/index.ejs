						<article class="product-box_data">
							<h2>Vans Old Skull</h2>
							<p>El modelo para todos</p>
							<p>Comentarios: 3</p>
						</article>
					</a>
				</section>
			</div>
		</div>
	</div>
	<!-- /Novedades -->
	<!-- Más comentados -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Más comentados</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="product.ejs">
						<figure class="product-box_image">
							<img src="../public/images/newBalance2002.jpeg" alt="cafetera moulinex">
						</figure>
						<article class="product-box_data">
							<h2>New Balance 2002 R</h2>
							<p>Increíble diseño y comodidad</p>
							<p>Comentarios: 3</p>
